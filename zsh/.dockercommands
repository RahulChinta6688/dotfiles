alias dklogin='container=$(docker ps | tail -n 1 | cut -b 1-12) && docker exec -it $container /bin/sh'
alias dkps='docker ps'
alias dklogs='container=$(docker ps | tail -n 1 | cut -b 1-12) && docker logs $container'
alias dkkill='container=$(docker ps | tail -n 1 | cut -b 1-12) && docker kill $container'
